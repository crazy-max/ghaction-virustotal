    "analysis",
    "malware",
    "security"
  ],
  "author": "CrazyMax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "axios": "^0.20.0",
    "form-data": "^3.0.0",
    "mime": "^2.4.6"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/jest": "^26.0.14",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.11.2",
    "@vercel/ncc": "^0.24.1",
    "dotenv": "^8.2.0",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "jest-runtime": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
