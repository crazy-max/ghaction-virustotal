    "mime": "^2.4.4"
  },
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^25.2.1",
    "@types/mime": "^2.0.1",
    "@types/node": "^13.11.1",
    "@zeit/ncc": "^0.22.1",
    "dotenv": "^8.2.0",
    "jest": "^25.4.0",
    "jest-circus": "^25.5.4",
    "jest-runtime": "^25.4.0",
    "prettier": "^2.0.4",
