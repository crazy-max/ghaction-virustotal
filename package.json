{
  "name": "virustotal-github-action",
  "description": "GitHub Action to upload and scan files with VirusTotal",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "test": "jest --coverage",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "cleanup-paths": "removeNPMAbsolutePaths ./ --force --fields _where _args",
    "pre-checkin": "npm run format && npm run cleanup-paths && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazy-max/ghaction-virustotal.git"
  },
  "keywords": [
    "actions",
    "virustotal",
    "analysis",
    "malware",
    "security"
  ],
  "author": "CrazyMax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^2.2.0",
    "axios": "^0.19.2",
    "form-data": "^3.0.0",
    "matcher": "^3.0.0",
    "mime": "^2.4.5"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/jest": "^25.2.3",
    "@types/mime": "^2.0.2",
    "@types/node": "^13.13.5",
    "@zeit/ncc": "^0.22.1",
    "dotenv": "^8.2.0",
    "jest": "^25.5.4",
    "jest-circus": "^25.5.4",
    "jest-runtime": "^25.5.4",
    "prettier": "^2.0.4",
    "removeNPMAbsolutePaths": "^2.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  }
}
